{
    "name": "gonetone/laravel-boost-windsurf-extension",
    "description": "Laravel Boost extension package that provides Windsurf editor and Windsurf JetBrains plugin integration.",
    "keywords": [
        "laravel",
        "boost",
        "ai",
        "development",
        "dev",
        "windsurf",
        "jetbrains",
        "phpstorm",
        "editor",
        "ide"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "GoneTone",
            "homepage": "https://github.com/GoneTone"
        }
    ],
    "support": {
        "issues": "https://github.com/GoneTone/laravel-boost-windsurf-extension/issues",
        "source": "https://github.com/GoneTone/laravel-boost-windsurf-extension"
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.45.3|^12.41.1",
        "laravel/boost": "^2.1.6"
    },
    "require-dev": {
        "laravel/pint": "^1.27.0"
    },
    "autoload": {
        "psr-4": {
            "GoneTone\\LaravelBoostWindsurf\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GoneTone\\LaravelBoostWindsurf\\WindsurfServiceProvider"
            ]
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint",
        "pint-test": "vendor/bin/pint --test"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
